    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.20.0",
    "caniuse-lite": "^1.0.30001477",
    "classnames": "^2.2.6",
    "env-cmd": "^10.1.0",
    "file-saver": "^2.0.2",
    "jszip": "^3.2.2",
    "lodash": "^4.17.11",
    "mobile-detect": "^1.4.3",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-idle-timer": "^4.5.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.0.1",
    "react-spinners": "^0.5.4",
    "redux": "^4.0.4",
    "rxjs": "^6.6.3",
    "typescript": "3.5.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf ./dist",
    "build:stage": "env-cmd -f .env.stage react-scripts build",
    "build:prod": "react-scripts build",
    "postbuild:stage": "mkdir -p dist/$(jq .name package.json -r) && mv build/* dist/$(jq .name package.json -r) && rm -rf build",
    "postbuild:prod": "mkdir -p dist/$(jq .name package.json -r) && mv build/* dist/$(jq .name package.json -r) && rm -rf build",
    "test": "CI=true react-scripts test --env=./src/__test__/custom-test-env.js",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "!deploy.stage": "------------- deploy to stage -------------",
    "predeploy.stage": "npm run clean && npm run build:stage",
    "deploy.stage": "./scripts/deploy.sh stage",
    "!deploy.prod": "------------- deploy to prod -------------",
    "predeploy.prod": "npm run clean && npm run build:prod",
    "deploy.prod": "./scripts/deploy.sh prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lemoncloud/ade-backend-api": "^0.23.327",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.8",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.135",
    "@types/moment": "^2.13.0",
    "@types/query-string": "^6.3.0",
    "@types/react-custom-scrollbars": "^4.0.5",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-redux": "^7.1.0",
    "@types/uuid": "^3.4.4",
    "prettier": "^1.17.1",
    "query-string": "^6.13.1",
    "redux-devtools": "^3.5.0"
  }
}
